{
    "name": "khamdullaevuz/laravel-slug",
    "description": "This is my package laravel-slug",
    "keywords": [
        "khamdullaevuz",
        "laravel",
        "laravel-slug"
    ],
    "homepage": "https://github.com/khamdullaevuz/laravel-slug",
    "license": "MIT",
    "authors": [
        {
            "name": "khamdullaevuz",
            "email": "elbek.khamdullaev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.0|^7.4",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/support": "^9.0|^8.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Khamdullaevuz\\LaravelSlug\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Khamdullaevuz\\LaravelSlug\\LaravelSlugServiceProvider"
            ],
            "aliases": {
                "LaravelSlug": "Khamdullaevuz\\LaravelSlug\\Facades\\LaravelSlug"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
